{
  // Vue Directives Enhanced
  "v-for": {
    "prefix": "v-for",
    "body": [
      "v-for=\"(${1:item}, ${2:index}) in ${3:items}\""
    ],
    "description": "Expects: Array | Object | number | string"
  },
  "v-for-with-key": {
    "prefix": "v-for-with-key",
    "body": [
      "v-for=\"(${1:item}, ${2:index}) in ${3:items}\" :key=\"${4:index}\""
    ],
    "description": "Expects: Array | Object | number | string"
  },
  "v-on": {
    "prefix": "v-on",
    "body": [
      "v-on:${1:event}=\"${2:handler}\""
    ],
    "description": "Expects: Function | Inline Statement"
  },
  "v-bind": {
    "prefix": "v-bind",
    "body": [
      "v-bind:${1:prop}=\"${2:value}\""
    ],
    "description": "Expects: any (with argument) | Object (without argument)"
  },
  // Vue Common Attrs
  "to": {
